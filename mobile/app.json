{
  "expo": {
    "name": "Academic View",
    "slug": "AcademicView",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#00c652"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "CAMERA",
        "ACCESS_FINE_LOCATION",
        "READ_EXTERNAL_STORAGE",
        "MANAGE_DOCUMENTS",
        "INTERNET",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "package": "com.deividreinke.AcademicView"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
